<script>
   alert("Hello World")
        document.write("<center>")
        document.write("<font color='red' size='5'>SENAI/SUMARÉ </font>")
        document.write("<br>")
        document.write("<img src='veio.jpg' width='15%'>")
        document.write("<br>")
        $nome = prompt("Qual o seu nome?")
        document.write("O nome digitado foi: " + $nome)
        document.write("<br>")
        $sexo = prompt("Qual é o seu sexo? [M] ou [F]")
        $idade = prompt("Qual é a sua idade?")

    if (($sexo == "M") || ($sexo == "m")) 
    {
        document.writeln(" = Masculino")
    }
    else 
    {
        document.writeln(" = Feminino")
    }

    document.write("<br>")

    document.write("Você é = " + $idade)

    if (($idade < "18")) 
    {
        document.writeln(" = Menor Idade")
    }
    else 
    {
        document.writeln(" = Maior Idade")
    }

    
    if (($sexo == "M") || ($sexo == "m"))
        {
            if (($idade < "18")) 
        {
            alert("menino")
        }
        else 
        {
            alert("homem")
        }
        }
        else
        {
            if (($idade < "18")) 
        {
            alert("menina")
        }
        else 
        {
            alert("mulher")
        }  
        }
        
        
  </SCRIPT>
